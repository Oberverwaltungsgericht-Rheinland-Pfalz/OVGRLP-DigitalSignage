{
  "name": "ds-suite",
  "license": "MIT",
  "scripts": {
    "build:displays": "ng build --project displays --prod --aot --base-href /displays/",
    "build:dsmanager": "ng build --project dsmanager --prod --aot --base-href /dsmanager/",
    "build:roomcontrol": "ng build --project roomcontrol --prod --aot --base-href /roomcontrol/",
    "build": "npm run build:displays && npm run build:roomcontrol && npm run build:dsmanager",
    "serve:displays": "ng serve --project displays --port 4201",
    "serve:dsmanager": "ng serve --project dsmanager --port 4203",
    "serve:roomcontrol": "ng serve --project roomcontrol --port 4202",
    "serve": "concurrently \"npm run serve:displays\" \"npm run serve:roomcontrol\" \"npm run serve:dsmanager\"",
    "serve:ovg": "concurrently \"npm run json-srv:ovg\" \"npm run serve\"",
    "serve:olg": "concurrently \"npm run json-srv:olg\" \"npm run serve\"",
    "json-srv:ovg": "json-server --watch json-srv/ovg/db.json --routes json-srv/ovg/routes.json --port 4200",
    "json-srv:olg": "json-server --watch json-srv/olg/db.json --routes json-srv/olg/routes.json --port 4200",
    "help": "./node_modules/.bin/nx help"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.1",
    "@angular/cdk": "^6.2.1",
    "@angular/common": "6.0.1",
    "@angular/compiler": "6.0.1",
    "@angular/core": "6.0.1",
    "@angular/flex-layout": "^6.0.0-beta.16",
    "@angular/forms": "6.0.1",
    "@angular/platform-browser": "6.0.1",
    "@angular/platform-browser-dynamic": "6.0.1",
    "@angular/router": "6.0.1",
    "@clr/angular": "^0.12.8",
    "@clr/icons": "^0.12.8",
    "@clr/ui": "^0.12.8",
    "@ctrl/ngx-codemirror": "^1.3.8",
    "@ngrx/effects": "5.2.0",
    "@ngrx/router-store": "5.2.0",
    "@ngrx/store": "5.2.0",
    "@ngrx/store-devtools": "5.2.0",
    "@nrwl/nx": "6.0.4",
    "@webcomponents/custom-elements": "^1.0.0",
    "angular2-moment": "^1.9.0",
    "breeze-bridge2-angular": "^1.1.0",
    "breeze-client": "^1.7.1",
    "codemirror": "^5.40.0",
    "core-js": "^2.5.4",
    "ngrx-store-freeze": "0.2.2",
    "rxjs": "6.0.0",
    "rxjs-compat": "6.0.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.1",
    "@angular-devkit/build-ng-packagr": "~0.6.1",
    "@angular/cli": "6.0.1",
    "@angular/compiler-cli": "6.0.1",
    "@angular/language-service": "6.0.1",
    "@ngrx/schematics": "5.2.0",
    "@nrwl/schematics": "6.0.4",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "concurrently": "~3.5.1",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "0.3.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0-rc.2",
    "prettier": "1.10.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "^2.7.2"
  }
}
