{
  "name": "ds-suite",
  "license": "MIT",
  "scripts": {
    "build:displays": "ng build --project displays --prod --aot --base-href /displays/ && jszip-cli add --force --output dist/displays.zip dist/apps/displays",
    "build:dsmanager": "ng build --project dsmanager --prod --aot --base-href /dsmanager/ && jszip-cli add --force --output dist/dsmanager.zip dist/apps/dsmanager",
    "build:roomcontrol": "ng build --project roomcontrol --prod --aot --base-href /roomcontrol/ && jszip-cli add --force --output dist/roomcontrol.zip dist/apps/roomcontrol",
    "build": "npm run build:displays && npm run build:roomcontrol && npm run build:dsmanager",
    "serve:displays": "ng serve --project displays --port 4201",
    "serve:dsmanager": "ng serve --project dsmanager --port 4203",
    "serve:roomcontrol": "ng serve --project roomcontrol --port 4202",
    "serve": "concurrently \"npm run serve:displays\" \"npm run serve:roomcontrol\" \"npm run serve:dsmanager\"",
    "serve:ovg": "concurrently \"npm run json-srv:ovg\" \"npm run serve\"",
    "serve:olg": "concurrently \"npm run json-srv:olg\" \"npm run serve\"",
    "json-srv:ovg": "json-server --watch json-srv/ovg/db.json --routes json-srv/ovg/routes.json --port 4200",
    "json-srv:olg": "json-server --watch json-srv/olg/db.json --routes json-srv/olg/routes.json --port 4200",
    "postinstall": "ngcc",
    "help": "./node_modules/.bin/nx help"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.2.4",
    "@angular/cdk": "^7.2.15",
    "@angular/common": "^10.2.4",
    "@angular/compiler": "^10.2.4",
    "@angular/core": "^10.2.4",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "^10.2.4",
    "@angular/platform-browser": "^10.2.4",
    "@angular/platform-browser-dynamic": "^10.2.4",
    "@angular/router": "^10.2.4",
    "@clr/angular": "^4.0.12",
    "@clr/core": "^3.0.0",
    "@clr/icons": "^4.0.12",
    "@clr/ui": "^4.0.12",
    "@ctrl/ngx-codemirror": "^2.2.1",
    "@ngrx/effects": "^10.1.2",
    "@ngrx/router-store": "10.1.2",
    "@ngrx/store": "10.1.2",
    "@ngrx/store-devtools": "10.1.2",
    "@nrwl/nx": "7.8.5",
    "@webcomponents/webcomponentsjs": "^2.0.0",
    "breeze-bridge2-angular": "^1.1.0",
    "breeze-client": "^1.7.2",
    "codemirror": "^5.59.4",
    "core-js": "^2.6.12",
    "moment": "^2.29.1",
    "ngx-moment": "^3.5.0",
    "rxjs": "^6.6.6",
    "rxjs-compat": "6.0.0",
    "tslib": "^2.0.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.3",
    "@angular-devkit/build-ng-packagr": "~0.1002.3",
    "@angular/cli": "^10.2.3",
    "@angular/compiler-cli": "^10.2.4",
    "@angular/language-service": "^10.2.4",
    "@ffflorian/jszip-cli": "^2.3.1",
    "@ngrx/schematics": "10.1.2",
    "@nrwl/schematics": "7.8.5",
    "@types/jasmine": "^2.8.17",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "@webcomponents/custom-elements": "^1.0.0",
    "codelyzer": "^6.0.1",
    "concurrently": "~3.5.1",
    "jasmine-core": "~3.5.0",
    "jasmine-marbles": "0.4.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.1.0",
    "prettier": "1.10.2",
    "protractor": "~7.0.0",
    "ts-node": "~5.0.1",
    "tslint": "~6.1.0",
    "typescript": "^4.0.7"
  }
}
