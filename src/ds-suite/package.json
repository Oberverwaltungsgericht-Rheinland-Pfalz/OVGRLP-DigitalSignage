{
  "name": "ds-suite",
  "license": "MIT",
  "scripts": {
    "build:displays": "ng build --project displays --prod --aot --base-href /displays/",
    "build:dsmanager": "ng build --project dsmanager --prod --aot --base-href /dsmanager/",
    "build:roomcontrol": "ng build --project roomcontrol --prod --aot --base-href /roomcontrol/",
    "build": "npm run build:displays && npm run build:roomcontrol && npm run build:dsmanager",
    "serve:displays": "ng serve --project displays --port 4201",
    "serve:dsmanager": "ng serve --project dsmanager --port 4203",
    "serve:roomcontrol": "ng serve --project roomcontrol --port 4202",
    "serve": "concurrently \"npm run serve:displays\" \"npm run serve:roomcontrol\" \"npm run serve:dsmanager\"",
    "serve:ovg": "concurrently \"npm run json-srv:ovg\" \"npm run serve\"",
    "serve:olg": "concurrently \"npm run json-srv:olg\" \"npm run serve\"",
    "json-srv:ovg": "json-server --watch json-srv/ovg/db.json --routes json-srv/ovg/routes.json --port 4200",
    "json-srv:olg": "json-server --watch json-srv/olg/db.json --routes json-srv/olg/routes.json --port 4200",
    "help": "./node_modules/.bin/nx help"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^7.2.15",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/flex-layout": "^7.0.0-beta.24",
    "@angular/forms": "^7.2.15",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/router": "^7.2.15",
    "@clr/angular": "^1.1.3",
    "@clr/icons": "^1.1.3",
    "@clr/ui": "^1.1.3",
    "@ctrl/ngx-codemirror": "^1.3.10",
    "@ngrx/effects": "^7.4.0",
    "@ngrx/router-store": "7.4.0",
    "@ngrx/store": "7.4.0",
    "@ngrx/store-devtools": "7.4.0",
    "@nrwl/nx": "7.8.5",
    "@webcomponents/custom-elements": "^1.2.4",
    "breeze-bridge2-angular": "^1.1.0",
    "breeze-client": "^1.7.1",
    "codemirror": "^5.46.0",
    "core-js": "^2.6.5",
    "moment": "^2.24.0",
    "ngrx-store-freeze": "0.2.2",
    "ngx-moment": "^3.4.0",
    "rxjs": "^6.5.1",
    "rxjs-compat": "6.0.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular-devkit/build-ng-packagr": "^0.13.9",
    "@angular/cli": "^7.3.9",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/language-service": "^7.2.15",
    "@ngrx/schematics": "7.4.0",
    "@nrwl/schematics": "7.8.5",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "concurrently": "~3.5.1",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "0.3.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.5",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.6",
    "prettier": "1.10.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tsickle": "^0.35.0",
    "tslib": "^1.9.3",
    "tslint": "~5.9.1",
    "typescript": "^3.2.4"
  }
