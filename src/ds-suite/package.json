{
  "name": "ds-suite",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run json-srv:run\" \"npm run displays:serve\"",
    "start:njzko": "concurrently \"npm run json-srv:njzko\" \"npm run displays:serve\" \"npm run roomcontrol:serve\"",
    "build": "ng build --prod --aot --base-href /displays/",
    "test": "ng test",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "displays:serve": "ng serve --project displays --port 4201",
    "roomcontrol:serve": "ng serve --project roomcontrol --port 4202",
    "displays:build": "ng build --project displays --prod --base-href /displays/",
    "roomcontrol:build": "ng build --project roomcontrol --prod --base-href /roomcontrol/",
    "json-srv:run": "json-server --watch json-srv/db/example.json --routes json-srv/routes.json --port 4200",
    "json-srv:njzko": "json-server --watch json-srv/db/njzko.json --routes json-srv/routes.json --port 4200",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "./node_modules/.bin/nx update",
    "update:check": "./node_modules/.bin/nx update:check",
    "update:skip": "./node_modules/.bin/nx update:skip",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.1",
    "@angular/cdk": "^6.2.1",
    "@angular/common": "6.0.1",
    "@angular/compiler": "6.0.1",
    "@angular/core": "6.0.1",
    "@angular/flex-layout": "^6.0.0-beta.16",
    "@angular/forms": "6.0.1",
    "@angular/platform-browser": "6.0.1",
    "@angular/platform-browser-dynamic": "6.0.1",
    "@angular/router": "6.0.1",
    "@ngrx/effects": "5.2.0",
    "@ngrx/router-store": "5.2.0",
    "@ngrx/store": "5.2.0",
    "@ngrx/store-devtools": "5.2.0",
    "@nrwl/nx": "6.0.4",
    "core-js": "^2.5.4",
    "ngrx-store-freeze": "0.2.2",
    "rxjs": "6.0.0",
    "rxjs-compat": "6.0.0",
    "zone.js": "^0.8.26",
    "@clr/angular": "^0.12.8",
    "@clr/ui": "^0.12.8",
    "@clr/icons": "^0.12.8",
    "@webcomponents/custom-elements": "^1.0.0"
  },
  "devDependencies": {
    "@angular/compiler-cli": "6.0.1",
    "@angular-devkit/build-ng-packagr": "~0.6.1",
    "@angular-devkit/build-angular": "~0.6.1",
    "ng-packagr": "^3.0.0-rc.2",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "typescript": "2.7.2",
    "@angular/cli": "6.0.1",
    "@angular/language-service": "6.0.1",
    "@ngrx/schematics": "5.2.0",
    "@nrwl/schematics": "6.0.4",
    "jasmine-marbles": "0.3.1",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "prettier": "1.10.2",
    "concurrently": "~3.5.1"
  }
}
