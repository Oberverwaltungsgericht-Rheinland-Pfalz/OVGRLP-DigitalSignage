  "dependencies": {
    "ag-grid": "*",
    "angular": "*",
    "angular-animate": "*",
    "angular-aria": "*",
    "angular-material": "*",
    "breeze-client": "*",
    "ui-router": "*"
  },
  "exportsOverride": {
